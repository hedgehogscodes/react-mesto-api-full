{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": ["eslint:recommended",  "airbnb-base"],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules":{
    "no-console": 0,
    "no-underscore-dangle": ["error", {
       "allow": ["_id"]
    }],
    "quotes": ["error", "double"]
  }

}